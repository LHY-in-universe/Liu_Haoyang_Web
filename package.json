{
  "name": "vue-app",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "vue-tsc --noEmit && vite build",
    "preview": "vite preview",
    "type-check": "vue-tsc --noEmit",
    "deploy": "npm run build && gh-pages -d dist",
    "lint": "eslint . --ext .vue,.js,.ts,.jsx,.tsx",
    "lint:fix": "eslint . --ext .vue,.js,.ts,.jsx,.tsx --fix",
    "format": "prettier --write \"src/**/*.{js,ts,vue,css,scss,md}\" \"*.{js,ts,json,md}\""
  },
  "dependencies": {
    "@types/dompurify": "^3.0.5",
    "@vueuse/core": "^13.9.0",
    "@vueuse/head": "^2.0.0",
    "dompurify": "^3.2.7",
    "highlight.js": "^11.11.1",
    "katex": "^0.16.23",
    "marked": "^16.3.0",
    "marked-katex-extension": "^5.1.5",
    "pinia": "^3.0.3",
    "vue": "^3.5.21",
    "vue-router": "^4.5.1"
  },
  "devDependencies": {
    "@types/node": "^24.6.2",
    "@typescript-eslint/eslint-plugin": "^8.45.0",
    "@typescript-eslint/parser": "^8.45.0",
    "@vitejs/plugin-vue": "^6.0.1",
    "eslint": "^9.37.0",
    "eslint-config-prettier": "^10.1.8",
    "eslint-plugin-vue": "^10.5.0",
    "gh-pages": "^6.3.0",
    "prettier": "^3.6.2",
    "typescript": "^5.9.3",
    "vite": "^7.1.7",
    "vue-tsc": "^3.1.0"
  }
}
